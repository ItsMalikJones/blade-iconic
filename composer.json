{
    "name": "itsmalikjones/blade-iconic",
    "description": "A package to easily make use of Iconic icons in your Laravel Blade views",
    "keywords": [
        "Blade",
        "Iconic",
        "Laravel"
    ],
    "homepage": "https://github.com/itsmalikjones/blade-iconic",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/itsmalikjones/blade-iconic/issues",
        "source": "https://github.com/itsmalikjones/blade-iconic"
    },
    "authors": [
        {
            "name": "Malik Alleyne-Jones",
            "homepage": "https://twitter.com/ItsMalikJones"
        }
    ],
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-icons": "^1.0",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "ItsMalikJones\\BladeIconic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ItsMalikJones\\BladeIconic\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ItsMalikJones\\BladeIconic\\BladeIconicServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
